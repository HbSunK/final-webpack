{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage", //按需注入
                "corejs": 3 //新版本需要指定核心库版本
            }
        ]
    ],
    "plugins": [
        "transform-vue-jsx",
        ["@babel/plugin-transform-runtime", {
            "corejs": 3
        }],
        [
            "import",
            {
                "libraryName": "vant",
                "libraryDirectory": "es",
                "style": true
            }
        ]
    ]
}


